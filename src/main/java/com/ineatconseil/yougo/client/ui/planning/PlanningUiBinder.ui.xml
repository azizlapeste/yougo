<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:y="urn:com.ineatconseil.yougo.client.ui.common.utils">
	<g:HTMLPanel>
		<g:Label addStyleNames="title" ui:field="planningTitleLabel" />
		<table cellspacing='0' cellpadding='0' class="panel">
			<tr>
				<td class="subTitle" style="text-align: left;">
					<g:InlineLabel ui:field="dateLabel" />
				</td>
				<td class="subTitle">
					<g:InlineLabel ui:field="helloLabel" />&nbsp;&nbsp;&nbsp;&nbsp;
					<g:Anchor ui:field="changePasswordLink" />&nbsp;&nbsp;&nbsp;&nbsp;
					<g:Anchor ui:field="disconnectLink" />
				</td>
			</tr>
			<tr>
				<td colspan="2">
					<g:DecoratedTabPanel ui:field='planningTabPanel'
						animationEnabled='true' />
				</td>
			</tr>
			<tr>
				<td colspan="2">
					<g:Button ui:field="newRequestButton" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder>